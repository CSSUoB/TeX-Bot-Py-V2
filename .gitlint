[general]
ignore = B5,B6
regex-style-search = True
fail-without-commits = True

[ignore-by-author-name]
regex = \A.*\[bot\].*\Z
ignore = T1,B1,B5,B6

[ignore-by-title]
regex = \AMerge\s+[a-f0-9]{8,64}\s+into\s+[a-f0-9]{8,64}\Z
ignore = T1,B5,B6

[title-must-not-contain-word]
words = fuck,stupid,annoying,rubbish,shit,crap
